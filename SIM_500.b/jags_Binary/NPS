model{	

   for(i in 1 : N) {
      
      ## T: trt[i]==1, C+HC0+HC1: trt[i]==0
      ## TrialMorpheus: extS==1, EC_IMvigor210+EC_IMvigor211 (HC0+HC1): extS==2
      
      Y[i] ~ dbern(p[i])
      logit(p[i]) <- alpha[extS[i]] + beta * trt[i] # p is the likelihood
      }
      
      ### prior 

     beta ~ dnorm(0, 0.001)

     alpha[1] ~ dnorm(0, 0.001)
     alpha[2] ~ dnorm(alpha[1],tau1)

     tau1 ~ dgamma(1, 0.001)

     logOR_TC=beta
      }